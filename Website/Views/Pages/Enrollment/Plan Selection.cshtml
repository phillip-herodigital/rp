<script type="text/ng-template" id="enrollmentZipToAddressPicker">
    <div>
        <div class="modal-header">
            <a href="" ng-click="$dismiss()">Close</a>
            <h2>@Html.Sitecore().Field("Zip to Address Header")</h2>
        </div>
        <div class="modal-body">
            <div ng-if="isDuplicateAddress(modal.selectedLocation.address)">
                <div class="notice error">
                    @Html.Sitecore().Field("Duplicate Address Message Text")
                </div>
                <hr class="content-divider" />
            </div>
            @Html.Sitecore().Field("Zip to Address Body")
            @using (var currentService = Html.ClientRepeater(m => m.Services, "0"))
            {
                <div class="item" @currentService.Validation().ErrorClass(s => s.Location)>
                    <label for="@currentService.Fix(h => h.IdFor(s => s.Location))">Address</label>
                    @currentService.TextBoxFor(s => s.Location, new 
                    { 
                        @class="typeahead",
                        ng_model="modal.selectedLocation",
                        placeholder=@Html.Sitecore().Field("Address Placeholder"),
                        typeahead = "location as location.address|address for location in getLocation(modal.serviceState,  $viewValue, true)",
                        ng_disabled="modal.serviceState == ''",
                        typeahead_loading="modal.loadingServiceAddress",
                        ng_class="{'loading' : modal.loadingServiceAddress}", 
                        autocomplete="on",
                        ng_init = "modal.selectedLocation = ''"
                    })
                    @currentService.ValidationMessageFor(s => s.Location)
                </div>
            }
        </div>
        <div class="modal-footer">
            <button ng-click="modal.save()" ng-disabled="(!modal.selectedLocation.address.line1 && !location.address.length == 1) || isDuplicateAddress(modal.selectedLocation.address)">@Html.Sitecore().Field("Continue Button")</button>
            <button class="secondary" ng-click="$dismiss()">@Html.Sitecore().Field("Cancel Button")</button>
        </div>
    </div>
</script>

<script type="text/ng-template" id="mustMoveInModal">
