                        <a class="button" data-btn-radio="'{{plan.id}}'" data-uncheckable ng-model="selectedOffers.gasPlan" href="">@Html.Sitecore().Field("Gas Button")</a>
                    </td>
                </tr>
                <tr class="details-row additional-fields">
                    <td colspan="6">
                        <p>{{ plan.description }}</p>
                    </td>
                </tr>
            </tbody>
        </table>
    </article>
</script>


