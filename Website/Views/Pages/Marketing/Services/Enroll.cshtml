@{ 
    var visitor = Sitecore.Analytics.Tracker.CurrentVisit;

    var selectedTab = "";
    
    switch (visitor.Region)
    {
        case "TX":
            selectedTab = "texas";
            break;
        case "GA":
            selectedTab = "georgia";
            break;
        case "PA":
            selectedTab = "pennsylvania";
            break;
        case "MD":
            selectedTab = "maryland";
            break;
        case "NJ":
            selectedTab = "newjersey";
            break;
        case "NY":
            selectedTab = "newyork";
            break;
        case "DC":
            selectedTab = "washingtondc";
            break;
        default:
            selectedTab = "notDetected";
            break;
    }
}
<article class="marketing state-tabs states-enroll" state-tabs="" ng-init="pane = '@selectedTab'">
    <ul class="tabs-nav">
        <li ng-click="selectPane('texas')" ng-class="{ active: pane == 'texas' }">
            <a href="">
                <span><i class="icon-state-texas"></i></span>
                @Html.Sitecore().Field("Texas State Text")
            </a>
        </li>
        <li ng-click="selectPane('georgia')" ng-class="{ active: pane == 'georgia' }">
            <a href="">
                <span><i class="icon-state-georgia"></i></span>
                @Html.Sitecore().Field("Georgia State Text")
            </a>
        </li>
        <li ng-click="selectPane('pennsylvania')" ng-class="{ active: pane == 'pennsylvania' }">
