        <li ng-click="selectPane('washingtondc')" ng-class="{ active: pane == 'washingtondc' }">
            <a href="">
                <span><i class="icon-state-washingtondc"></i></span>
                @Html.Sitecore().Field("Washington DC State Text")
            </a>
        </li>
    </ul>
    <div class="tab-content" ng-class="{ hidden : pane != 'texas' }">
        <div class="tab-header">
            <div class="wrapper">
                <p>
                    @Html.Sitecore().Field("Texas Links")
                </p>
            </div>
        </div>
        <article class="marketing wrapper enroll">
            <div class="text">
                <h1>@Html.Sitecore().Field("Texas State Text")</h1>
                @Html.Sitecore().Field("Texas Content")
            </div>
        </article>
        <div class="tab-footer"></div>
    </div>
