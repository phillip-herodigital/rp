@{
    foreach (Item item in Model)
    {
        var url = Sitecore.Links.LinkManager.GetItemUrl(item);
        <li>
            <a href="@url">@Html.Raw(Sitecore.Web.UI.WebControls.FieldRenderer.Render(item, "Image", "w=185"))</a>
            <h4><a href="@url">@Html.Sitecore().Field("First Name", item)</a></h4>
            <h2><a href="@url">@Html.Sitecore().Field("Last Name", item)</a></h2>
            <p>@Html.Sitecore().Field("Location", item)</p>
        </li>
    }
}
