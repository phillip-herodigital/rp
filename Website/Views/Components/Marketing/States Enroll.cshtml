            <a href="">
                <span><i class="icon-state-washingtondc"></i></span>
                @Html.Sitecore().Field("Washington DC State Text")
            </a>
        </li>
    </ul>
    <div class="tab-content" ng-class="{ hidden : pane != 'texas' }">
        <div class="tab-header">
            <div class="wrapper">
                <p>
                    @Html.Sitecore().Field("Texas View Rates Link")
                    <a href="/enrollment?BC_ID=1&RefSite=SCS&CO_LA=US_EN&State=TX&AccountType=R&ServiceType=Ele" target="_blank" class="button green">@Html.Sitecore().Field("Enroll Now Text")</a>
                </p>
            </div>
        </div>
    </div>
