@{
    var currentID = Sitecore.Context.Item.ID;
    var onlineAccount = ((Sitecore.Data.Fields.LinkField)Html.Sitecore().CurrentItem.Fields["Online Account Information Link"]);
    var accountInfo = ((Sitecore.Data.Fields.LinkField)Html.Sitecore().CurrentItem.Fields["Account Information Link"]);
    var notifications = ((Sitecore.Data.Fields.LinkField)Html.Sitecore().CurrentItem.Fields["Notifications and Reminders Link"]);
    var enrolledAccounts = ((Sitecore.Data.Fields.LinkField)Html.Sitecore().CurrentItem.Fields["Enrolled Accounts Link"]);
}

<article class="subnav">
    <ul>
        @(currentID == onlineAccount.TargetItem.ID ? Html.Raw("<li class='active'>" + onlineAccount.Text + "</li>") : Html.Raw("<li>" + Html.Sitecore().Field("Online Account Information Link") + "</li>"))
        @(currentID == accountInfo.TargetItem.ID ? Html.Raw("<li class='active'>" + accountInfo.Text + "</li>") : Html.Raw("<li>" + Html.Sitecore().Field("Account Information Link") + "</li>"))
        @(currentID == notifications.TargetItem.ID ? Html.Raw("<li class='active'>" + notifications.Text + "</li>") : Html.Raw("<li>" + Html.Sitecore().Field("Notifications and Reminders Link") + "</li>"))
        @(currentID == enrolledAccounts.TargetItem.ID ? Html.Raw("<li class='active'>" + enrolledAccounts.Text + "</li>") : Html.Raw("<li>" + Html.Sitecore().Field("Enrolled Accounts Link") + "</li>"))
    </ul>
</article>