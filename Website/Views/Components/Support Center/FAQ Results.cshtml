@{ 
    var FAQs = ViewData["Faqs"];
}
<div class="results-content" ng-show="searchResults" ng-cloak>
    <a href="" ng-click="backToSupport(@FAQs)">Back to Support</a>
    <p>Showing {{resultsPageRange[resultsPage].low + 1}} - {{resultsPageRange[resultsPage].high + 1}} of {{faqs.length}} search results for “{{searchData.text}}” in {{searchData.category}}<span ng-show="searchData.state.name"> - {{searchData.state.abbreviation}}</span></p>
    <div class="pagination clearfix">
        <div class="back"><a href="" ng-click="setResultsPage(0)">First</a></div>
        <div class="back"><a href="" ng-click="setResultsPage(resultsPage - 1)">Previous</a></div>
        <div class="page-number" ng-repeat="page in resultsPageRange" ng-class="{selected: resultsPage == $index}" ng-click="setResultsPage($index)">{{$index + 1}}</div>
        <div class="forward"><a href="" ng-click="setResultsPage(resultsPage + 1)">Next</a></div>
        <div class="forward"><a href="" ng-click="setResultsPage(resultsPageRange.length - 1)">Last</a></div>
        <div>
            Results Per Page <select ng-model="resultsPerPage.value" ng-change="setResultsPerPage()">
                <option ng-repeat="value in resultsPerPage.options">{{value}}</option>
            </select>
        </div>
    </div>
</div>