<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeGlobalFilters, Sitecore.Mvc']"
           type="StreamEnergy.Sitecore.RegisterPlatformBundles, StreamEnergy.Core" />
        <processor type="StreamEnergy.Sitecore.InitializeUnityContainer, StreamEnergy.Core" />
        <processor type="StreamEnergy.Sitecore.RegisterMvcFactories, StreamEnergy.Core" />
      </initialize>
    </pipelines>
  </sitecore>
</configuration>