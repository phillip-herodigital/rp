<?xml version="1.0" encoding="utf-8" ?> 
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <Command>           
    <Border Padding="0px 32px 0px 0px" RenderAs="Literal">
      <Inline Class="scCommand" ID="$ID" Click="$Click" RollOver="true" ToolTip="$ToolTip">
        <ThemedImage Src="$Icon" Width="24" Height="24" Margin="0px 6px 0px 6px" Align="absmiddle"/>
        <Literal Class="scClickFont" Text="$Header"/>
      </Inline>
    </Border>
  </Command>
</control>
