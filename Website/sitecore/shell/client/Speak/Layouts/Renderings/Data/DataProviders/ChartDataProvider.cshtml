@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Speak.Charting.sitecore.shell.client.Speak.Layouts.Renderings.Data.DataProviders
@using Sitecore.Web.UI.Controls.Common.UserControls
@model RenderingModel
@{
  var userControl = this.Html.Sitecore().Controls().GetUserControl(this.Model.Rendering);
  userControl.Requires.Script("controls", "ChartDataProvider.js");
  var cssClass = "sc-ChartDataProvider";
  
  DataProviderHelper.SetAttributes(userControl);
  
  var dateFrom = userControl.GetString("DateFrom");
  var dateTo = userControl.GetString("DateTo");  
  var resolution = userControl.GetString("Resolution");
  var resolutionRange = userControl.GetString("ResolutionRange");
  
  userControl.Attributes["data-sc-datefrom"] = dateFrom;
  userControl.Attributes["data-sc-dateto"] = dateTo;    
  userControl.Attributes["data-sc-resolution"] = resolution;
  userControl.Attributes["data-sc-resolutionRange"] = resolutionRange;
    
  var htmlAttributes = userControl.HtmlAttributes;
  userControl.AppendClass(cssClass);
}
<script type='text/x-sitecore-chartdataprovider' @htmlAttributes> </script>