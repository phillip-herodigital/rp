@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Web.UI.Controls.Common.UserControls
@model RenderingModel
@{
  Sitecore.Web.UI.Controls.Common.UserControls.UserControl rendering = Html.Sitecore().Controls().GetUserControl(Model.Rendering);
  rendering.Class = "sc-reportupdatewatcher";
  rendering.Requires.Script("ecm", "ReportUpdateWatcher.js");

  rendering.GetString("Language", "language");
  rendering.GetString("MessageId", "messageId");
}
<div @rendering.HtmlAttributes></div>